{
    "name": "liateam/api-exceptions",
    "description": "Exception list using in api which help you to easy manage your api exceptions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mojtaba Rakhisi",
            "email": "mojtaba.rakhisi@gmail.com"
        }
    ],
    "require": {
        "liateam/api-response": "^2.0"
    },
    "repositories": {
        "liateam/api-response" : {
            "type": "vcs",
            "url": "git@git.liateam.net:php/packages/api-response.git"
        }
    },
    "autoload": {
        "psr-4": {
            "Liateam\\ApiExceptions": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Liateam\\ApiExceptions\\Providers"
            ]
        }
    }
}
